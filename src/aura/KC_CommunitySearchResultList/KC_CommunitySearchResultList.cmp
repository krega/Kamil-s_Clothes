<aura:component description="KC_CommunitySearchResultList"
                implements="forceCommunity:availableForAllPageTypes,lightning:isUrlAddressable"
                access="global" controller="KC_CommunitySearchController">
    <aura:attribute name="stringQuery" type="String"/>
    <aura:attribute name="results" type="Object[]"/>

    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <div class="slds-grid slds-gutters">
        <aura:iteration var="item" items="{!v.results}" indexVar="index">
    <div class="slds-col">

     <c:KC_CommunitySearchResult item="{!item}"/>

    </div>
        </aura:iteration>
    </div>

</aura:component>